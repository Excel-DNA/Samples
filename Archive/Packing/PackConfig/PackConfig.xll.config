<configuration>
   <system.diagnostics>
      <switches>
         <add name="General" value="4" />
      </switches>
      <trace autoflush="true" indentsize="2">
         <listeners>
            <add name="myListener" 
		 type="System.Diagnostics.TextWriterTraceListener" 
		 initializeData="C:\PackConfig.log" 
		 traceOutputOptions="DateTime, Timestamp, ProcessId, ThreadId, LogicalOperationStack, Callstack" />
         </listeners>
      </trace>
   </system.diagnostics>
</configuration>
